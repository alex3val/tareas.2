<!DOCTYPE html>
<html>
<head>
  <title>Calificación en palabras</title>
</head>
<body>
<?php
  $nota = 5; 
  $calificacion; 

  switch (true) {
    case ($nota >= 5 && $nota <= 6):
      $calificacion = "Aprobado";
      break;
    case ($nota >= 7 && $nota <= 8):
      $calificacion = "Notable";
      break;
    case ($nota >= 9 && $nota <= 10):
      $calificacion = "Sobresaliente";
      break;
    default:
      $calificacion = "Nota inválida";
  }

  echo $calificacion;
?>
</body>
</html>
